{
    "name": "ukr cyrillic to latin",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Translates ukrainian cyrillic script into latin script.",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Ukrainian Latin"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "tabs"
    ]
}